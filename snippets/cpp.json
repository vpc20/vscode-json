{
	// Place your snippets for cpp here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console": {
		"prefix": "out",
		"body": [
			"cout << $1 << endl;"
		],
		"description": "Print to console"
	},
	"Function with no parameter": {
		"prefix": "func0",
		"body": [
			"${1:rettype} ${2:funcname} () {",
			"${3:  /*code*/}",
			"}"
		],
		"description": "Function with no parameter"
	},
	"Function with 1 parameter": {
		"prefix": "func1",
		"body": [
			"${1:rettype} ${2:funcname} (${3:datatype1} ${4:param1}) {",
			"${5:  /*code*/}",
			"}"
		],
		"description": "Function with 1 parameter"
	},
	"Function with 2 parameter": {
		"prefix": "func2",
		"body": [
			"${1:rettype} ${2:funcname} (${3:datatype1} ${4:param1}, ${5:datatype2} ${6:param2}) {",
			"${7:  /*code*/}",
			"}"
		],
		"description": "Function with 2 parameter"
	},
	"Function with 3 parameter": {
		"prefix": "func3",
		"body": [
			"${1:rettype} ${2:funcname} (${3:datatype1} ${4:param1}, ${5:datatype2} ${6:param2}, ${7:datatype3} ${8:param3}) {",
			"${9:  /*code*/}",
			"}"
		],
		"description": "Function with 3 parameter"
	}
}